{
	"name": "login-manager",
	"version": "1.2.1",
	"private": true,
	"description": "Store all your website logins at one place",
	"productName": "Login Manager",
	"author": {
		"name": "Mohamed Alaa",
		"email": "mohamed.alaawo@gmail.com",
		"url": "https://github.com/mohammed-alaa/login-manager"
	},
	"scripts": {
		"format": "prettier --write \"{main,preload,src}/**/*.{js,ts,vue}\"",
		"lint": "yarn format && eslint --ext .js,.ts,.vue --ignore-path .gitignore --fix src",
		"dev": "electron-vite dev",
		"preview": "yarn lint && electron-vite preview",
		"build": "yarn lint && electron-vite build && electron-builder --config electron-builder.json",
		"postinstall": "electron-builder install-app-deps",
		"postuninstall": "electron-builder install-app-deps"
	},
	"main": "build/main/index.js",
	"dependencies": {
		"aes256": "^1.1.0",
		"bcrypt": "^5.1.1"
	},
	"devDependencies": {
		"@vitejs/plugin-vue": "^4.4.0",
		"@vue/eslint-config-prettier": "^8.0.0",
		"autoprefixer": "^10.4.16",
		"electron": "^13.0.0",
		"electron-builder": "^24.6.4",
		"electron-devtools-installer": "^3.1.0",
		"electron-vite": "^1.0.28",
		"eslint": "8",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-vue": "8",
		"postcss": "^8.4.31",
		"prettier": "^3.0.3",
		"sass": "^1.69.5",
		"tailwindcss": "^3.3.5",
		"vite": "^4.5.0",
		"vite-plugin-electron": "^0.14.1",
		"vite-plugin-eslint": "^1.8.1",
		"vue": "^3.3.7",
		"vue-router": "^4.2.5",
		"vuex": "^4.1.0"
	}
}
