	},
	"scripts": {
		"format": "prettier -w -u \"src/**/*.{js,ts,vue}\"",
		"lint": "yarn format && eslint --ext .js,.ts,.vue --ignore-path .gitignore --fix src",
		"dev": "electron-vite dev -w",
